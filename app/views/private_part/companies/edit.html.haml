%p= link_to 'Back',manage_company_path(company),class: 'btn btn-sm btn-primary'
.col-md-10
  = render 'shared/error_msg', object: company
  = form_for company,url: manage_company_path, method: :put  do |f|
    = render 'form', f: f
    .col-md-8
      .form-group
        = f.submit 'Update', class: 'btn btn-success'
